pipeline {
    agent any
    tools {
        maven 'M2_HOME'
        stage("checkout scm"){
            steps{
                checkout scm
            }
        }
        stage("sonacloud analysis"){
            steps{
                sh 'mvn verify org.sonarsource.scanner.maven:sonar-maven-plugin:sonar -Dsonar.projectKey=teammy44u_geo-app'
            }
        }
    }
}